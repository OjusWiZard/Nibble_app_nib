<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nib_one_row"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:padding="5dp"
    android:background="@drawable/ic_rectangle_18"
    >


    <ImageView
        android:id="@+id/nib_git6"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="228dp"
        app:layout_constraintEnd_toEndOf="@+id/nib_git5"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="@+id/nib_git5"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_git5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_linkedIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toEndOf="@+id/nib_linkedIn"
        app:layout_constraintTop_toTopOf="@+id/nib_linkedIn"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_git4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="228dp"
        app:layout_constraintEnd_toEndOf="@+id/nib_linkedIn"
        app:layout_constraintHorizontal_bias="0.706"
        app:layout_constraintStart_toStartOf="@+id/nib_linkedIn"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_git2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="228dp"
        app:layout_constraintEnd_toEndOf="@+id/nib_git"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_git"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_image"
        android:layout_width="83dp"
        android:layout_height="69dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_work"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nib_name"
        app:layout_constraintVertical_bias="0.586"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/nib_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:text="Name"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.17" />

    <TextView
        android:id="@+id/nib_club"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Club"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_name"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352" />

    <TextView
        android:id="@+id/nib_work"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Programmer"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_club"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <TextView
        android:id="@+id/nib_year"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="I - YEAR"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/nib_image"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_image"
        app:layout_constraintTop_toBottomOf="@+id/nib_image"
        app:layout_constraintVertical_bias="0.556" />

    <ImageView
        android:id="@+id/nib_git"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="176dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_work"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_linkedIn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        app:layout_constraintBottom_toTopOf="@+id/nib_git2"
        app:layout_constraintStart_toEndOf="@+id/nib_git"
        app:layout_constraintTop_toTopOf="@+id/nib_git"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_logo_linkedin" />


</androidx.constraintlayout.widget.ConstraintLayout>
